{
    "name": "@siposdani87/expo-rich-text-editor",
    "version": "0.9.1",
    "description": "This rich text editor written in TypeScript and use React Hooks structure. This component use the HTML ContentEditable div feature and React communicate and send data to native JavaScript via WebView.",
    "author": "DaÌniel Sipos",
    "license": "MIT",
    "homepage": "https://siposdani87.com",
    "private": false,
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "keywords": [
        "expo-rich-text-editor",
        "richtext",
        "editor"
    ],
    "files": [
        "dist",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/siposdani87/expo-rich-text-editor.git"
    },
    "bugs": {
        "url": "https://github.com/siposdani87/expo-rich-text-editor/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prebuild": "npm run clean",
        "build": "npx babel src --out-dir dist --extensions \".ts,.tsx\" -d",
        "clean": "rm -rf dist",
        "lint": "npx eslint . --ext .ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "npx prettier --write 'src/**/*.ts*'"
    },
    "peerDependencies": {
        "react": ">=18.0",
        "react-native": "*",
        "react-native-webview": "*"
    },
    "dependencies": {
        "@babel/cli": "^7.19.3",
        "expo": "^47.0.0",
        "metro-react-native-babel-transformer": "^0.73.3",
        "react": ">=18.0",
        "react-native": "*",
        "react-native-webview": "*"
    },
    "devDependencies": {
        "@babel/plugin-transform-typescript": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@react-native-community/eslint-config": "^3.0.1",
        "@types/jest": "^27.0.2",
        "@types/react": "~18.0.24",
        "@types/react-native": "~0.70.6",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.21.0",
        "prettier": "^2.7.1",
        "typescript": "^4.6.3"
    }
}
